{
  "restApiRoot": "/verso/api",
  "host": "0.0.0.0",
  "port": 3001,
  "swagger": {
    "protocol": "https"
  },
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "500kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "500kb"
    },
    "cors": false
  },
  "httpMode": false,
  "certConfig": {
    "path": "/certificate/path/",
    "key": "privkey.pem",
    "cert": "fullchain.pem",
    "ca": [],
    "requestCert": false,
    "rejectUnauthorized": false
  }
}
